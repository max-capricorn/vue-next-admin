{
  "name": "vue-next-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix  ./",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "element-plus": "^2.2.8",
    "pinia": "^2.0.14",
    "vue": "^3.2.16",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.4.3",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,vue}": [
      "npm run lint:script",
      "git add ."
    ]
  }
}
